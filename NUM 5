#include <iostream>
using namespace std;
int main() {
    int s, t; 
    int a, b; 
    int m, n; 
    cin >> s >> t >> a >> b >> m >> n;
    int apple, orange;
    int appleCount = 0, orangeCount = 0;
    for (int i = 0; i < m; i++) {
        cin >> apple;
        if (a + apple >= s && a + apple <= t) {
            appleCount++;
        }
    }
    for (int i = 0; i < n; i++) {
        cin >> orange;
        if (b + orange >= s && b + orange <= t) {
            orangeCount++;
        }
    }
    cout << appleCount << endl;
    cout << orangeCount << endl;
    return 0;
}
